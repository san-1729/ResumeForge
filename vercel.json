{
  "buildCommand": "rm -rf .vercel/output && pnpm run build:vercel && node scripts/fix-vercel-build.js",
  "devCommand": "pnpm run dev",
  "installCommand": "pnpm install",
  "framework": "remix",
  "outputDirectory": "build/client",
  "git": {
    "deploymentEnabled": {
      "main": false,
      "feature/linkedin-profile-import": true
    }
  },
  "env": {
    "NODE_ENV": "production",
    "REMIX_ENTRY_SERVER_PATH": "./entry.server.node.tsx",
    "VERCEL_FORCE_NO_BUILD_CACHE": "1"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ]
}
